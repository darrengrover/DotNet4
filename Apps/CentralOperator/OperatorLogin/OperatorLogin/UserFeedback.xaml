<Window x:Class="OperatorLogin.UserFeedback"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UserFeedback" Height="360" Width="802" WindowStyle="None" ResizeMode="NoResize" Loaded="Window_Loaded" Background="#FFDFDFDF" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="56" />
            <RowDefinition Height="80*" />
            <RowDefinition Height="2*" />
            <RowDefinition Height="80*" />
            <RowDefinition Height="2*" />
            <RowDefinition Height="80*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="420*" />
            <ColumnDefinition Width="120*" />
        </Grid.ColumnDefinitions>
        <Button Content="OK" Grid.Column="1" Grid.Row="1" Height="Auto" HorizontalAlignment="Stretch" Margin="10,8,10,10" Name="btnOK" VerticalAlignment="Stretch" Width="Auto" IsDefault="False" Click="btnOK_Click" FontSize="24" Panel.ZIndex="1" TouchDown="btnOK_TouchDown" />
        <Button Content="Cancel" Height="Auto" HorizontalAlignment="Stretch" Margin="10" Name="btnCancel" VerticalAlignment="Stretch" Width="Auto" Grid.Column="1" Grid.Row="3" IsCancel="False" Click="btnCancel_Click" FontSize="24" Panel.ZIndex="1" TouchDown="btnCancel_TouchDown" />
        <TextBox Grid.Row="1" Height="Auto" HorizontalAlignment="Stretch" Margin="8,8,0,8" Name="txtFeedback" VerticalAlignment="Stretch" Width="Auto" Grid.RowSpan="6" FontSize="20" Panel.ZIndex="1" IsReadOnly="True" TextWrapping="Wrap" />
        <Label Content="Label" Height="Auto" HorizontalAlignment="Stretch" Margin="4" Name="lblHeader" VerticalAlignment="Stretch" Grid.ColumnSpan="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" FontSize="24" FontWeight="Normal" Panel.ZIndex="1" Foreground="White" Padding="5" />
        <Rectangle Grid.RowSpan="7" Height="Auto" HorizontalAlignment="Stretch" Margin="0" Name="rectangle1" Stroke="Black" VerticalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="2" Fill="#FF005B8A" />
        <Rectangle Grid.ColumnSpan="2" Grid.RowSpan="1" Height="Auto" HorizontalAlignment="Stretch" Margin="0" Name="rectangle2" Stroke="Black" VerticalAlignment="Stretch" Width="Auto" Fill="#FFE32524" />
        <Button Content="Stop" FontSize="24" Height="Auto" HorizontalAlignment="Stretch" IsCancel="False" Margin="10,10,10,12" Name="btnStop" Panel.ZIndex="1" VerticalAlignment="Stretch" Width="Auto" Grid.Column="1" Grid.Row="5" Click="btnStop_Click" TouchDown="btnStop_TouchDown" />
    </Grid>
</Window>
